{
    "name": "fwolfsjaeger/doctrine-cockroachdb-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine DBAL CockroachDB Driver Bundle for Symfony",
    "homepage": "https://github.com/fwolfsjaeger/doctrine-cockroachdb-bundle",
    "license": "MIT",
    "version": "0.8.1",
    "keywords": [
        "pdo_pgsql",
        "pdo",
        "cockroach",
        "cockroachdb",
        "doctrine",
        "doctrine-cockroachdb",
        "doctrine-cockroachdb-bundle",
        "symfony"
    ],
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Florian Wolfsjaeger",
            "email": "florian@wolfsjaeger.at"
        }
    ],
    "require": {
        "php": "^8.0",
        "fwolfsjaeger/doctrine-cockroachdb": "^0.8",
        "doctrine/doctrine-bundle": "^2.10",
        "symfony/dependency-injection": "^6.0|6.1|6.2|6.3|^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "^3.41",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-symfony": "^1.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "DoctrineCockroachDB\\": "src"
        }
    }
}
